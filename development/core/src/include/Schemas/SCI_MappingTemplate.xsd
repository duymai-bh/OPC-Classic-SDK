	<xs:complexType name="SCI_OPC_SCIStatusTemplateType">
		<xs:annotation>
			<xs:documentation>Used for SCI Status OPC mapping</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RequestSwitchover" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>COMMAND - BOOL - Request Duty SCI Switchover</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsDuty" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>STATUS - BOOL - SCI Duty Status (False = Standby, True = Duty)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SCIState" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>STATUS - WORD - SCI Status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SCI_CommunicationAlarm" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>STATUS - BOOL - SCI Communications Alarm (False = Good, True = Bad).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
