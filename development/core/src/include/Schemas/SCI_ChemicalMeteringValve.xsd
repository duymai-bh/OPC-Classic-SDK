					<xs:documentation>Canbus Write Scan List Number to Send Command to SEM</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PosModeSDOList" type="uint8">
				<xs:annotation>
					<xs:documentation>Position Mode Command SDO List</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlowModeSDOList" type="uint8">
				<xs:annotation>
					<xs:documentation>Flow Mode (Regulated or Non-Regulated) Command SDO List</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommandSDOList" type="uint8">
				<xs:annotation>
					<xs:documentation>Command Command SDO List</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ZeroFlowSDOList" type="uint8">
				<xs:annotation>
					<xs:documentation>Zero Flow SDO List</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SCIEditData" type="SkoFloCMVEditDataType">
				<xs:annotation>
					<xs:documentation>This group contains all the CMV data that can be edited on the ETU HMI.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PIDDisplay" type="bool" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Item Displayed (True) or not displayed (False) on the ETU Process Identification (PID) mimic display.</xs:documentation>
