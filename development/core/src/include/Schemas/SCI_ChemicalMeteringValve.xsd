	</xs:complexType>
	<xs:complexType name="SkoFloCMVEditDataType">
		<xs:annotation>
			<xs:documentation>SCI Editable Data For A Sko Flo CMV</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FlowControlBasedOnDP" type="bool">
				<xs:annotation>
					<xs:documentation>If set the Flow Control Based on Differential Pressure, if not set Flow Control Based on Stem Position.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SelectedFlowTable">
				<xs:annotation>
					<xs:documentation>Lists the selected Flow Table number. Default Flow Table 1 - value 0.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="uint8">
						<xs:maxInclusive value="4"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NoAdjustments">
				<xs:annotation>
					<xs:documentation>Maximum number of allowed adjustments when the difference pressure method is specified. Default 3.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="uint8">
						<xs:maxInclusive value="15"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MinsBetweenAdjustments">
				<xs:annotation>
					<xs:documentation>Time, in minutes, to wait between each adjustment when the difference pressure method is specified. Factory default 5.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="uint8">
						<xs:maxInclusive value="240"/>
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
