	<xs:complexType name="SCI_Modbus_DualValveTemplateType">
		<xs:annotation>
			<xs:documentation>Used for Modbus mapping of valve</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Open" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>COMMAND - BOOL - Open Command From Interface</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Close" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>COMMAND - BOOL - Close Command From Interface</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CloseDCVTargetPresDisable" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>PROPERTY - BOOL - If Set The Valve Logic Ignores The Target Pressures When Determining The Valves Status.
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OpenDCVTargetPresDisable" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>PROPERTY - BOOL - If Set The Valve Logic Ignores The Target Pressures When Determining The Valves Status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PressuriseTargetFlowDisable" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>PROPERTY - BOOL - Pressurise Phase If Set The Valve Logic Ignores The Target Flow When Determining The Valves Status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Active" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>STATUS - BOOL - This Sci Active For This Valve</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="SCI_DataType">
				<xs:annotation>
					<xs:documentation>STATUS - UINT16 - Status Word</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
