	<xs:complexType name="SensorModbusInterfacesTypes">
		<xs:choice>
			<xs:element name="HalliburtonDHPT" type="HaliburtonDHPTType">
				<xs:annotation>
					<xs:documentation>Interface to Halliburton 2nd Generation IWIS Subsea Interface Card For ROC gauges (Controls and Initialisation). As defined within Haliburton Document 96705068 Rev B</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FramoVxMeter" type="FramoVxMeterType">
				<xs:annotation>
					<xs:documentation>Interface to Support Framo Vx Meter (WGFM/MPFM) (Controls and Initialisation). As defined within Framo Document DOC-0018268 Rev B</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BHDHPT" type="BHDHPTControlType">
				<xs:annotation>
					<xs:documentation>Baker Hughes DHPT control type.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GenericModbusDevice" type="GenericModbusType">
				<xs:annotation>
					<xs:documentation>Interface to support generic modbus devices.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="HaliburtonDHPTType">
		<xs:annotation>
			<xs:documentation>Halliburton 2nd Generation IWIS Subsea Interface Card</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="string32">
				<xs:annotation>
					<xs:documentation>Name of the Interface (Max 32 characters).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Tag" type="string32" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer tag reference</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ModbusDeviceName" type="string32">
				<xs:annotation>
					<xs:documentation>Name of associated modbus slave device</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NoOfGauges" type="string32">
				<xs:annotation>
					<xs:documentation>Number of Gauges Configured  (Range 1 to 10).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FirmwareRevision" type="string32" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reference to Analogue Parameter for Firmware Revision</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SCIEditData" type="HaliburtonDHPTEditDataType">
				<xs:annotation>
					<xs:documentation>This group contains all the Hallibutron data that can be edited on the ETU HMI.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--The following are used by the ETU to Display the Data on P&ID (if req.)-->
