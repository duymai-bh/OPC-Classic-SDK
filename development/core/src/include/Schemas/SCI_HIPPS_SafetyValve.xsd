<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="SCI_SurfaceTypes.xsd"/>
	<xs:include schemaLocation="BaseTypes.xsd"/>
	<xs:include schemaLocation="SCI_DeviceMapping.xsd"/>
	<xs:complexType name="HIPPS_SafetyValveType">
		<xs:annotation>
			<xs:documentation>HIPPS Safety Valve S39500-001-01-301</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="string32">
				<xs:annotation>
					<xs:documentation>Tree valve name of the HIPPS Safety valve (Max 32 characters).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Tag" type="string32" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer Tag Reference</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DSM" type="string32">
				<xs:annotation>
					<xs:documentation>DSM ( DCV (Directional Control Valve) Support Module) (Max 32 characters).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SSM" type="string32">
				<xs:annotation>
					<xs:documentation>SSM (Sensor Support Module) (Max 32 characters).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Channel" type="DCVChannelType">
				<xs:annotation>
					<xs:documentation>DCV channel type.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FailureMode" type="ValveFailureModeType">
				<xs:annotation>
					<xs:documentation>Valve Failure Mode Type.</xs:documentation>
				</xs:annotation>
			</xs:element>

			<!--
			HIPPS Safety Valve Specific sensors
			-->
			<xs:element name="ActPresSen1" type="string32">
				<xs:annotation>
					<xs:documentation>Actuator Pressure Sensor Definition. (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActPresSen2" type="string32">
				<xs:annotation>
					<xs:documentation>Actuator Pressure Sensor Definition. (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>

			<!--
			HIPPS Profile Specific sensors
			-->
			<xs:element name="PrfActPresSen1" type="string32">
				<xs:annotation>
					<xs:documentation>Actuator Pressure Sensor Definition. (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfActPresSen2" type="string32">
				<xs:annotation>
					<xs:documentation>Actuator Pressure Sensor Definition. (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfPos1Sen" type="string32">
				<xs:annotation>
					<xs:documentation>Position Indicator (LVDT) 1 Sensor Definition. (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfPos2Sen" type="string32">
				<xs:annotation>
					<xs:documentation>Position Indicator (LVDT) 2 Sensor Definition. (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfTypeTB" type="string32">
				<xs:annotation>
					<xs:documentation>Profiler Type in Progress</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfState" type="string32">
				<xs:annotation>
					<xs:documentation>Profiler State</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfRunOnCountRemaining" type="string32">
				<xs:annotation>
					<xs:documentation>Run On Count Remaining</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfSampleRequestReplyCount" type="string32">
				<xs:annotation>
					<xs:documentation>Sample Request Reply Count</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfPos1DisableTB" type="string32">
				<xs:annotation>
					<xs:documentation>Disable position sensor 1 tailback</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfPos2DisableTB" type="string32">
				<xs:annotation>
					<xs:documentation>Disable position sensor 2 tailback</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfActPres1DisableTB" type="string32">
				<xs:annotation>
					<xs:documentation>Disable actuator sensor 1 tailback</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfActPres2DisableTB" type="string32">
				<xs:annotation>
					<xs:documentation>Disable actuator sensor 2 tailback</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfDiscreteDisableTB" type="string32">
				<xs:annotation>
					<xs:documentation>Disable Discrete tailback</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="PrfActPres1Index" type="ProfileAnalogueIndexType">
				<xs:annotation>
					<xs:documentation>Index of actuator 1 in CmdHippsStartProfiler</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfActPres2Index" type="ProfileAnalogueIndexType">
				<xs:annotation>
					<xs:documentation>Index of actuator 2 in CmdHippsStartProfiler</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfPos1Index" type="ProfileAnalogueIndexType">
				<xs:annotation>
					<xs:documentation>Index of Position Sensor 1 in CmdHippsStartProfiler</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfPos2Index" type="ProfileAnalogueIndexType">
				<xs:annotation>
					<xs:documentation>Index of Position Sensor 2 in CmdHippsStartProfiler</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfExchangeID" type="uint8">
				<xs:annotation>
					<xs:documentation>Exchange ID used for HIPPS profiling</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfDiscrSensor" type="string32">
				<xs:annotation>
					<xs:documentation>Profiler Discrete Sensor</xs:documentation>
				</xs:annotation>
			</xs:element>

			<!--
			HIPPS related sensor readbacks
			-->
			<xs:element name="HippsTripStatus" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>


			<!--
			PST related sensor readbacks
			-->
			<xs:element name="PST_InProgress" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_MaxTimeTB" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_TargetPosTB" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_TimeOut" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_LowestPos1" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_LowestPos2" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_InitEventTime" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_TimeDuration" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_ReqTB" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_MaxTimeReject" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_TargetPosReject" type="string32">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_OutputSel" type="bool">
				<xs:annotation>
					<xs:documentation>Partial Output Selection</xs:documentation>
				</xs:annotation>
			</xs:element>

            <!--
			Generic Standard Hydraulic parameters
			-->
            <xs:element name="SupPresSen" type="string32">
				<xs:annotation>
					<xs:documentation>Hydraulic Supply Channel Sensor Definition. (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RetPresSen" type="string32">
				<xs:annotation>
					<xs:documentation>Hydraulic Return Channel Sensor Definition.  (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HasFlowmeters" type="bool">
				<xs:annotation>
					<xs:documentation>True if flowmeters fitted to SCM.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupMeter" type="string32">
				<xs:annotation>
					<xs:documentation>Supply Meter, flow meter which measures the quanity of hydraulic fluid used during the valve open operation.  (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RetMeter" type="string32">
				<xs:annotation>
					<xs:documentation>Return Meter, flow meter which measures the quanity of hydraulic fluid vented from the valve during the valve close operation.  (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Pos1Sen" type="string32" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Position Indicator (LVDT) 1 Sensor Definition. (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Pos2Sen" type="string32" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Position Indicator (LVDT) 2 Sensor Definition. (This is a named reference to a Sensor defined in the Sensors Block)</xs:documentation>
				</xs:annotation>
			</xs:element>


			<xs:element name="SCIEditData" type="HIPPS_SafetyValveEditDataType">
				<xs:annotation>
					<xs:documentation>This group contains all the HIPPS Safety valve data that can be edited on the ETU HMI.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--			The following are used by the ETU to Display the Data on P&ID (if req.)-->
			<xs:element name="PIDDisplay" type="bool" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Item Displayed (True) or not displayed (False) on the ETU Process Identification (PID) mimic display.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PIDLocation" type="int8" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Location on the ETU Process Identification (PID) mimic display. (Only Applicable if PIDDisplay is True).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SCI_Mapping" type="SCI_DeviceMappingType">
				<xs:annotation>
					<xs:documentation>Structure holding Modbus and OPC mapping data</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScanRate" type="uint32">
				<xs:annotation>
					<xs:documentation>The Rate At Which Continuous Scanning Is Performed In ms</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="HIPPS_SafetyValveEditDataType">
		<xs:annotation>
			<xs:documentation>SCI Editable Data For HIPPS Safety Valve S39500-001-01-301</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SolenoidDuration" type="DCVSolDurType">
				<xs:annotation>
					<xs:documentation>Soldenoid duration time in milliseconds, the time the solenoid is energised inside the DCV to initiatee valve operation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SolenoidDelay" type="DCVSolPreArmType">
				<xs:annotation>
					<xs:documentation>Delay after profiling begins before firing DCV</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Powersave" type="PowersaveType">
				<xs:annotation>
					<xs:documentation>Power save</xs:documentation>
				</xs:annotation>
			</xs:element>

			<!--
			 DCV params
			-->
			<xs:element name="OpenDuration" type="ValveDurType">
				<xs:annotation>
					<xs:documentation>the time taked for the valve open operaton to complete in seconds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CloseDuration" type="ValveDurType">
				<xs:annotation>
					<xs:documentation>the time taked for the valve close operaton to complete in seconds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetActPres1Disable" type="bool">
				<xs:annotation>
					<xs:documentation>If set the valve logic ignores the target actuator 1 pressures when determining the valves status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetActPres2Disable" type="bool">
				<xs:annotation>
					<xs:documentation>If set the valve logic ignores the target actuator 2 pressures when determining the valves status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetOpenPres" type="float">
				<xs:annotation>
					<xs:documentation>Target Open Pressure, this is the actuator pressure acheived the the valve has successfully opened.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetClosePres" type="float">
				<xs:annotation>
					<xs:documentation>Target Close Pressure, the vent line pressure when the valve is closed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetFlowDisable" type="bool">
				<xs:annotation>
					<xs:documentation>If set the valve logic ignores the flow reading when determining the valve status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetOpenFlow" type="float">
				<xs:annotation>
					<xs:documentation>Target Pressurise Flow, the amount of hydraulic fluid used performing the open operation, if valve opened successfully.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetCloseFlow" type="float">
				<xs:annotation>
					<xs:documentation>Target Close Flow, the amount of hydraulic fluid used performing the close operation, if valve closed successfully. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlowTolerance" type="float">
				<xs:annotation>
					<xs:documentation>When checking flow values for valves, the SCI should look for the flow being Target +/- Toolerance</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetPos1Disable" type="bool" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If set the valve logic ignores the Position Indicator 1 when determining the valve status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetPos2Disable" type="bool" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If set the valve logic ignores the Position Indicator 2 when determining the valve status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetOpenPos" type="float" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Target Open Position, this is the Threshold for the Position Indicator to be successfully opened.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetClosePos" type="float" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Target Close Position, this is the Threshold for the Position Indicator to be successfully Close.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupPresProfileDisable" type="bool" default="false">
				<xs:annotation>
					<xs:documentation>Disable collection of supply pressure profile</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RetPresProfileDisable" type="bool" default="false">
				<xs:annotation>
					<xs:documentation>Disable collection of return pressure profile</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupFlowProfileDisable" type="bool" default="true">
				<xs:annotation>
					<xs:documentation>Disable collection of supply flow profile</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RetFlowProfileDisable" type="bool" default="true">
				<xs:annotation>
					<xs:documentation>Disable collection of return flow profile</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Pos1ProfileDisable" type="bool" default="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Disable collection of Position Indicator 1 Profile</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Pos2ProfileDisable" type="bool" default="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Disable collection of Position Indicator 2 Profile</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActProfileDuration" type="ProfileDurType">
				<xs:annotation>
					<xs:documentation>Duration over which the valve actuator profile is taken.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PresProfileDuration" type="ProfileDurType">
				<xs:annotation>
					<xs:documentation>Pressure Profile Duration, duration of the valve profile, in seconds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlowProfileDuration" type="ProfileDurType">
				<xs:annotation>
					<xs:documentation>Flow Profile Duration, duration of the valve profile, in seconds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Pos1ProfileDuration" type="ProfileDurType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Position Indicator 1 Profile Duration, duration of the valve profile, in seconds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Pos2ProfileDuration" type="ProfileDurType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Position Indicator 2 Profile Duration, duration of the valve profile, in seconds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProfileDestinationFile" type="string255">
				<xs:annotation>
					<xs:documentation>Destination file for retrieved profiles</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PressuriseAlmPres" type="float">
				<xs:annotation>
					<xs:documentation>Static pressure alarm setpoint for pressurised valve</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VentAlmPres" type="float">
				<xs:annotation>
					<xs:documentation>Static pressure alarm setpoint for vented valve</xs:documentation>
				</xs:annotation>
			</xs:element>


			<!--
			Trigger profile settings
			-->
			<xs:element name="PrfRunOnCount" type="EGD_ProfileRunOnCountType">
				<xs:annotation>
					<xs:documentation>Run on Count</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfSampleInterval" type="EGD_ProfileSampleIntervalType">
				<xs:annotation>
					<xs:documentation>Sample Interval in millisecs</xs:documentation>
				</xs:annotation>
			</xs:element>

			<!--
			Trip (Discrete) Trigger transition
			-->
			<xs:element name="PrfTripLoToHi" type="bool">
				<xs:annotation>
					<xs:documentation>Discrete Transition</xs:documentation>
				</xs:annotation>
			</xs:element>

			<!--
			Trigger profile analogue settings
			Real units to be used for thresholds, but scaled to current in UDP message
			The number and names of sensors is fixed
			-->
			<xs:element name="PrfPos1Low" type="float">
				<xs:annotation>
					<xs:documentation>Position Sensor 1 Lo Threshold</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfPos1High" type="float">
				<xs:annotation>
					<xs:documentation>Position Sensor 1 Hi Threshold</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="PrfPos2Low" type="float">
				<xs:annotation>
					<xs:documentation>Position Sensor 2 Lo Threshold</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfPos2High" type="float">
				<xs:annotation>
					<xs:documentation>Position Sensor 2 Hi Threshold</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfPos1Disable" type="bool">
				<xs:annotation>
					<xs:documentation>Disable Position Sensor 1 Trigger</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfPos2Disable" type="bool">
				<xs:annotation>
					<xs:documentation>Disable Position Sensor 2 Trigger</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfActPres1Low" type="float">
				<xs:annotation>
					<xs:documentation> Actuator Sensor 1 Lo Threshold</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfActPres1High" type="float">
				<xs:annotation>
					<xs:documentation> Actuator Sensor 1 Hi Threshold</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="PrfActPres2Low" type="float">
				<xs:annotation>
					<xs:documentation> Actuator Sensor 2 Hi Threshold</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfActPres2High" type="float">
				<xs:annotation>
					<xs:documentation> Actuator Sensor 2 Lo Threshold</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfActPres1Disable" type="bool">
				<xs:annotation>
					<xs:documentation>Disable Actuator Sensor 1 Trigger</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrfActPres2Disable" type="bool">
				<xs:annotation>
					<xs:documentation>Disable Actuator Sensor 2 Trigger</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="PrfDiscreteDisable" type="bool">
				<xs:annotation>
					<xs:documentation>Disable Discrete Trigger</xs:documentation>
				</xs:annotation>
			</xs:element>

			<!--
			Partial Stroke Configurations
			-->
			<xs:element name="PST_InhibitPos1" type="bool">
				<xs:annotation>
					<xs:documentation>Partial Stroke inhbit VPT1</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_InhibitPos2" type="bool">
				<xs:annotation>
					<xs:documentation>Partial Stroke inhbit VPT2</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PST_TargetPos" type="float">
				<xs:annotation>
					<xs:documentation>Partial Stroke Target Position</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="PST_MaxTime" type="uint32">
				<xs:annotation>
