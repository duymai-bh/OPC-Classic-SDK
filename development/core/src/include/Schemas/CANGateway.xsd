<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">

	<xs:complexType name="CANGatewayType">
		<xs:annotation>
			<xs:documentation>The CAN Gateway config parameters</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IPPort" type="Private_IP_PortType" default="50000">
				<xs:annotation>
					<xs:documentation>The port number that the gateway will serve on.</xs:documentation>
				</xs:annotation>
			</xs:element>
    </xs:sequence>
	</xs:complexType>


</xs:schema>
